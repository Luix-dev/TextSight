{
	"name": "obsidian-sample-plugin",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@smui/button": "^7.0.0-beta.16",
		"@smui/card": "^7.0.0-beta.16",
		"@smui/paper": "^7.0.0-beta.16",
		"@smui/tab": "^7.0.0-beta.16",
		"@smui/tab-bar": "^7.0.0-beta.16",
		"@smui/tab-indicator": "^7.0.0-beta.16",
		"@smui/tab-scroller": "^7.0.0-beta.16",
		"@tsconfig/svelte": "^5.0.2",
		"@types/node": "^16.11.6",
		"@types/react": "^18.2.55",
		"@types/react-dom": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"autoprefixer": "^10.4.17",
		"builtin-modules": "3.3.0",
		"esbuild": "^0.20.0",
		"esbuild-svelte": "^0.8.0",
		"obsidian": "latest",
		"postcss": "^8.4.35",
		"sass": "^1.70.0",
		"smui-theme": "^7.0.0-beta.16",
		"svelte": "^4.2.10",
		"svelte-preprocess": "^5.1.3",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
